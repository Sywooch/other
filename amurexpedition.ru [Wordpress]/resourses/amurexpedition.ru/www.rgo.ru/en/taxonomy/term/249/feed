  &lt;div class=&quot;views-slideshow-cycle-main-frame-row-item views-row views-row-69 views-row-even&quot;&gt;
    
  &lt;div class=&quot;views-field views-field-field-image&quot;&gt;        &lt;div class=&quot;field-content&quot;&gt;&lt;a href=&quot;https://www.rgo.ru/sites/default/files/styles/full_view/public/node/12812/141200px.jpg?itok=EsVRc5FO&quot; title=&quot;&quot; class=&quot;colorbox&quot; data-colorbox-gallery=&quot;gallery-node-12812-f6FljYf6P3k&quot;&gt;&lt;img typeof=&quot;foaf:Image&quot; src=&quot;https://www.rgo.ru/sites/default/files/styles/gallery_main/public/node/12812/141200px.jpg?itok=ij4xL-Je&quot; width=&quot;429&quot; height=&quot;313&quot; alt=&quot;&quot; title=&quot;&quot; /&gt;&lt;/a&gt;&lt;div class=&quot;image-title&quot;&gt;&lt;/div&gt;&lt;/div&gt;  &lt;/div&gt;&lt;/div&gt;
