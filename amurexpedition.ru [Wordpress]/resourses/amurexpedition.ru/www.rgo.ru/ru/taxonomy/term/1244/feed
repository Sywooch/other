  &lt;div class=&quot;views-field views-field-field-image&quot;&gt;        &lt;div class=&quot;field-content&quot;&gt;&lt;a href=&quot;https://www.rgo.ru/sites/default/files/styles/full_view/public/node/16771/img-9985.jpg?itok=1YJLzNVQ&quot; title=&quot;&quot; class=&quot;colorbox&quot; data-colorbox-gallery=&quot;gallery-node-16771-BN8jLe7afsM&quot;&gt;&lt;img typeof=&quot;foaf:Image&quot; src=&quot;https://www.rgo.ru/sites/default/files/styles/gallery_main/public/node/16771/img-9985.jpg?itok=unAkHCzM&quot; width=&quot;470&quot; height=&quot;313&quot; alt=&quot;&quot; title=&quot;&quot; /&gt;&lt;/a&gt;&lt;div class=&quot;image-title&quot;&gt;&lt;/div&gt;&lt;/div&gt;  &lt;/div&gt;&lt;/div&gt;
