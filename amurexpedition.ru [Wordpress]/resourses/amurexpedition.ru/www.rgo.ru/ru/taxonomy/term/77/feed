    &lt;div class=&quot;views-content-field-image&quot;&gt;
    &lt;img typeof=&quot;foaf:Image&quot; src=&quot;https://www.rgo.ru/sites/default/files/styles/gallery_thumb/public/node/17525/img-9873.jpg?itok=7bU6lRWb&quot; width=&quot;99&quot; height=&quot;66&quot; alt=&quot;&quot; /&gt;  &lt;/div&gt;
&lt;/div&gt;
&lt;/li&gt;
&lt;li class=&quot;views-slideshow-jcarousel-pager-item views_slideshow_jcarousel_pager_item views-row-even&quot; id=&quot;views_slideshow_jcarousel_pager_item_top_photogallery-photogallery_linked1_4_7&quot;&gt;
  &lt;div class=&quot;views-field-field-image&quot;&gt;
    &lt;div class=&quot;views-content-field-image&quot;&gt;
    &lt;img typeof=&quot;foaf:Image&quot; src=&quot;https://www.rgo.ru/sites/default/files/styles/gallery_thumb/public/node/17525/img-9878.jpg?itok=-uOn5fx7&quot; width=&quot;99&quot; height=&quot;66&quot; alt=&quot;&quot; /&gt;  &lt;/div&gt;
&lt;/div&gt;
&lt;/li&gt;
&lt;li class=&quot;views-slideshow-jcarousel-pager-item views_slideshow_jcarousel_pager_item views-row-odd&quot; id=&quot;views_slideshow_jcarousel_pager_item_top_photogallery-photogallery_linked1_4_8&quot;&gt;
  &lt;div class=&quot;views-field-field-image&quot;&gt;
    &lt;div class=&quot;views-content-field-image&quot;&gt;
    &lt;img typeof=&quot;foaf:Image&quot; src=&quot;https://www.rgo.ru/sites/default/files/styles/gallery_thumb/public/node/17525/img-9887.jpg?itok=JbOs8m1v&quot; width=&quot;99&quot; height=&quot;66&quot; alt=&quot;&quot; /&gt;  &lt;/div&gt;
&lt;/div&gt;
&lt;/li&gt;
