&lt;p style=&quot;line-height: 0.58cm;&quot;&gt;&lt;span style=&quot;color: #000000;&quot;&gt;&lt;span style=&quot;font-size: large;&quot;&gt;&lt;span style=&quot;background: #ffffff;&quot;&gt;Запись на лекции: &lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;span style=&quot;color: #000080;&quot;&gt;&lt;span lang=&quot;zxx&quot; xml:lang=&quot;zxx&quot;&gt;&lt;span style=&quot;text-decoration: underline;&quot;&gt;&lt;a href=&quot;/ru/lektorii/moskovskiy-lektoriy&quot; class=&quot;western&quot;&gt;&lt;span style=&quot;color: #548dd4;&quot;&gt;&lt;span style=&quot;font-size: large;&quot;&gt;&lt;span lang=&quot;ru-RU&quot; xml:lang=&quot;ru-RU&quot;&gt;&lt;span style=&quot;background: #ffffff;&quot;&gt;http://www.rgo.ru/ru/lektorii/moskovskiy-lektoriy&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/a&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt; &lt;span style=&quot;color: #000080;&quot;&gt;&lt;span lang=&quot;zxx&quot; xml:lang=&quot;zxx&quot;&gt;&lt;span style=&quot;color: #000000;&quot;&gt;&lt;span style=&quot;font-size: large;&quot;&gt;&lt;span lang=&quot;ru-RU&quot; xml:lang=&quot;ru-RU&quot;&gt;&lt;span style=&quot;background: #ffffff;&quot;&gt;(открывается за 4-6 дней до лекции; по почте записи нет).&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
&lt;p style=&quot;margin-bottom: 0cm; line-height: 0.58cm;&quot;&gt;&lt;span style=&quot;color: #000000;&quot;&gt;&lt;span style=&quot;font-size: large;&quot;&gt;&lt;span style=&quot;background: #ffffff;&quot;&gt;Онлайн-трансляция мероприятий: &lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
&lt;p style=&quot;line-height: 0.58cm;&quot;&gt;&lt;span style=&quot;color: #000080;&quot;&gt;&lt;span lang=&quot;zxx&quot; xml:lang=&quot;zxx&quot;&gt;&lt;span style=&quot;text-decoration: underline;&quot;&gt;&lt;a href=&quot;/ru/lektorii/moskovskiy-lektoriy/onlayn&quot; class=&quot;western&quot;&gt;&lt;span style=&quot;color: #548dd4;&quot;&gt;&lt;span style=&quot;font-size: large;&quot;&gt;&lt;span lang=&quot;ru-RU&quot; xml:lang=&quot;ru-RU&quot;&gt;&lt;span style=&quot;background: #ffffff;&quot;&gt;http://www.rgo.ru/ru/lektorii/moskovskiy-lektoriy/onlayn&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/a&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;span style=&quot;color: #000000;&quot;&gt;&lt;span style=&quot;font-size: large;&quot;&gt;&lt;span style=&quot;background: #ffffff;&quot;&gt;.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
