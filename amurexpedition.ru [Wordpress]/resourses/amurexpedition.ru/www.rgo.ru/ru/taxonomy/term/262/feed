&lt;p class=&quot;MsoBodyText&quot; style=&quot;line-height: 16.5pt;&quot;&gt;&lt;span style=&quot;font-size: 14pt; background: white;&quot; lang=&quot;RU&quot; xml:lang=&quot;RU&quot;&gt;Запись на лекции: &lt;/span&gt;&lt;span lang=&quot;RU&quot; xml:lang=&quot;RU&quot;&gt;&lt;a href=&quot;/ru/lektorii/moskovskiy-lektoriy&quot; target=&quot;_blank&quot;&gt;&lt;span style=&quot;font-size: 14.0pt; mso-bidi-font-family: &#039;Times New Roman&#039;; color: #548dd4; background: white;&quot;&gt;http://www.rgo.ru/ru/lektorii/moskovskiy-lektoriy&lt;/span&gt;&lt;/a&gt; &lt;/span&gt;&lt;span class=&quot;MsoHyperlink&quot;&gt;&lt;span style=&quot;font-size: 14pt; background: white;&quot;&gt;(открывается за 4-6 дней до лекции; по почте записи нет).&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
&lt;p class=&quot;MsoBodyText&quot; style=&quot;margin-bottom: .0001pt; line-height: 16.5pt;&quot;&gt;&lt;span style=&quot;font-size: 14pt; background: white;&quot; lang=&quot;RU&quot; xml:lang=&quot;RU&quot;&gt;Онлайн-трансляция мероприятий: &lt;/span&gt;&lt;/p&gt;
&lt;p class=&quot;MsoBodyText&quot; style=&quot;line-height: 16.5pt;&quot;&gt;&lt;span lang=&quot;RU&quot; xml:lang=&quot;RU&quot;&gt;&lt;a href=&quot;/ru/lektorii/moskovskiy-lektoriy/onlayn&quot; target=&quot;_blank&quot;&gt;&lt;span style=&quot;font-size: 14.0pt; mso-bidi-font-family: &#039;Times New Roman&#039;; color: #548dd4; background: white;&quot;&gt;http://www.rgo.ru/ru/lektorii/moskovskiy-lektoriy/onlayn&lt;/span&gt;&lt;/a&gt;&lt;/span&gt;&lt;span style=&quot;font-size: 14pt; background: white;&quot; lang=&quot;RU&quot; xml:lang=&quot;RU&quot;&gt;.&lt;/span&gt;&lt;/p&gt;
&lt;p class=&quot;MsoBodyText&quot; style=&quot;line-height: 16.5pt;&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-size: 14pt; background: white;&quot; lang=&quot;RU&quot; xml:lang=&quot;RU&quot;&gt;Вход в здание открывается за 30 минут до начала!&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;
