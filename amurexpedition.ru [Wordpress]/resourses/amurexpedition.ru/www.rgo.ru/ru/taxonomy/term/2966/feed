  &lt;div class=&quot;views-field views-field-field-image&quot;&gt;        &lt;div class=&quot;field-content&quot;&gt;&lt;a href=&quot;https://www.rgo.ru/sites/default/files/styles/full_view/public/node/15122/zagustayskaya.jpg?itok=dTtps_Mx&quot; title=&quot;&quot; class=&quot;colorbox&quot; data-colorbox-gallery=&quot;gallery-node-15122-VMha0QUVcTs&quot;&gt;&lt;img typeof=&quot;foaf:Image&quot; src=&quot;https://www.rgo.ru/sites/default/files/styles/gallery_main/public/node/15122/zagustayskaya.jpg?itok=G6gUQRt4&quot; width=&quot;470&quot; height=&quot;264&quot; alt=&quot;&quot; title=&quot;&quot; /&gt;&lt;/a&gt;&lt;div class=&quot;image-title&quot;&gt;&lt;/div&gt;&lt;/div&gt;  &lt;/div&gt;&lt;/div&gt;
&lt;/div&gt;
