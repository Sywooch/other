    &lt;div id=&quot;views_slideshow_cycle_main_photogallery-photogallery_9&quot; class=&quot;views_slideshow_cycle_main views_slideshow_main&quot;&gt;&lt;div id=&quot;views_slideshow_cycle_teaser_section_photogallery-photogallery_9&quot; class=&quot;views-slideshow-cycle-main-frame views_slideshow_cycle_teaser_section&quot;&gt;
  &lt;div id=&quot;views_slideshow_cycle_div_photogallery-photogallery_9_0&quot; class=&quot;views-slideshow-cycle-main-frame-row views_slideshow_cycle_slide views_slideshow_slide views-row-1 views-row-odd&quot;&gt;
  &lt;div class=&quot;views-slideshow-cycle-main-frame-row-item views-row views-row-0 views-row-odd views-row-first&quot;&gt;
    
  &lt;div class=&quot;views-field views-field-field-image&quot;&gt;        &lt;div class=&quot;field-content&quot;&gt;&lt;/div&gt;  &lt;/div&gt;&lt;/div&gt;
