  &lt;div class=&quot;views-field views-field-field-headimage&quot;&gt;        &lt;div class=&quot;field-content&quot;&gt;&lt;img typeof=&quot;foaf:Image&quot; src=&quot;https://www.rgo.ru/sites/default/files/styles/head_image_article/public/wellman_air_ship_loc_ggbain.03344.jpg?itok=T3N4Mxa1&quot; width=&quot;470&quot; height=&quot;313&quot; alt=&quot;&quot; /&gt;&lt;div class=&quot;image-title&quot;&gt;&lt;/div&gt;&lt;/div&gt;  &lt;/div&gt;&lt;/div&gt;
