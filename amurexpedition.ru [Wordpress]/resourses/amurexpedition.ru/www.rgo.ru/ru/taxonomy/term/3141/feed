  &lt;div class=&quot;views-slideshow-cycle-main-frame-row-item views-row views-row-0 views-row-odd views-row-first&quot;&gt;
    
  &lt;div class=&quot;views-field views-field-field-headimage&quot;&gt;        &lt;div class=&quot;field-content&quot;&gt;&lt;img typeof=&quot;foaf:Image&quot; src=&quot;https://www.rgo.ru/sites/default/files/styles/head_image_article/public/node/17036/granty.jpg?itok=PtIUPR6v&quot; width=&quot;470&quot; height=&quot;313&quot; alt=&quot;&quot; /&gt;&lt;div class=&quot;image-title&quot;&gt;&lt;/div&gt;&lt;/div&gt;  &lt;/div&gt;&lt;/div&gt;
